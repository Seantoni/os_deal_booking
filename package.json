{
  "name": "os_deals_booking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' next dev --webpack",
    "dev:webpack": "NODE_OPTIONS='--max-old-space-size=8192' next dev",
    "build": "NEXT_DISABLE_GOOGLE_FONTS=1 next build",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING=1 prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:setup": "prisma generate && prisma migrate dev --name init",
    "db:seed:categories": "tsx prisma/seed-categories.ts",
    "db:migrate:settings": "tsx prisma/migrate-settings-to-db.ts",
    "db:import:businesses": "tsx prisma/import-businesses-csv.ts",
    "db:import:opportunities": "tsx prisma/import-opportunities-csv.ts",
    "db:migrate:payment-plan": "tsx prisma/migrate-payment-plan-values.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.953.0",
    "@aws-sdk/s3-request-presigner": "^3.953.0",
    "@clerk/localizations": "^3.30.1",
    "@clerk/nextjs": "^6.35.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/nextjs": "^10.33.0",
    "@sparticuz/chromium": "^131.0.1",
    "@thednp/dommatrix": "^2.0.12",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "browser-image-compression": "^2.0.2",
    "dotenv": "^17.2.3",
    "next": "^16.0.10",
    "openai": "^6.9.1",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "puppeteer-core": "^24.34.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "resend": "^6.5.2",
    "svix": "^1.84.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
